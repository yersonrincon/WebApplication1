@model WebApplication1.Models.proveedor

@{
    ViewBag.Title = "Details";
    var nombreProducto = Model.nombre;
}
<h2>Detalles del Proveedor: @nombreProducto</h2>

<div>
    
    <hr />
    <dl class="dl-horizontal">
        <dt>
            Nombre
        </dt>

        <dd>
            @Html.DisplayFor(model => model.nombre)
        </dd>

        <dt>
            Dirreccion
        </dt>

        <dd>
            @Html.DisplayFor(model => model.direccion)
        </dd>

        <dt>
            Telefono
        </dt>

        <dd>
            @Html.DisplayFor(model => model.telefono)
        </dd>

        <dt>
            Nombre contacto
        </dt>
        <dd>
            @Html.DisplayFor(model => model.nombre_contacto)
        </dd>

        <dd>
            @WebApplication1.Controllers.ProductoController.NombreProveedor(Model.id)
        </dd>
    </dl>
</div>
<p>
    
    @Html.ActionLink("Regresar a lista ", "Index")
</p>
